{
  "name": "@storefuse/adapter-woo-rest",
  "version": "0.0.1",
  "description": "WooCommerce REST API adapter for StoreFuse",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "storefuse",
    "woocommerce",
    "rest-api",
    "adapter"
  ],
  "author": "ProgrammerNomad",
  "license": "MIT",
  "dependencies": {
    "@woocommerce/woocommerce-rest-api": "^1.0.2"
  },
  "devDependencies": {
    "@storefuse/core": "workspace:*",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@storefuse/core": "^0.0.1"
  }
}
