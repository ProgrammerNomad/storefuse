{
  "name": "@storefuse/cli",
  "version": "0.1.0",
  "description": "StoreFuse CLI for scaffolding and managing projects",
  "type": "module",
  "bin": {
    "storefuse": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "prompts": "^2.4.2",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "execa": "^9.0.0",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/prompts": "^2.4.9",
    "@storefuse/core": "workspace:*",
    "tsup": "^8.0.2",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "storefuse",
    "cli",
    "woocommerce",
    "nextjs"
  ]
}
