{
  "name": "@storefuse/module-search",
  "version": "0.1.0",
  "description": "StoreFuse module for product search functionality",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "@storefuse/core": "workspace:*",
    "tsup": "^8.0.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "^18.2.0 || ^19.0.0"
  },
  "keywords": [
    "storefuse",
    "woocommerce",
    "search",
    "module"
  ]
}
